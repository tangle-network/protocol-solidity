    "setup:vanchor8": "./scripts/bash/groth16/vanchor/phase2_poseidon_vanchor8.sh",
    "setup:semaphore-bridge2": "./scripts/bash/groth16/semaphore/phase2_semaphore_bridge_2.sh",
    "setup": "./scripts/bash/setup_verifiers.sh",
    "test": "npx hardhat test"
  },
