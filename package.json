  "license": "LGPL-3.0",
  "scripts": {
    "compile": "npx hardhat compile",
    "build": "yarn compile && yarn build:circuits && yarn build:hashers && yarn build:ptau",
    "build:circuits": "./scripts/compile_circom.sh",
    "build:hashers": "ts-node scripts/build_hashers.ts",
    "build:ptau": "./scripts/generate_phase1_ptau.sh",
    "setup:poseidon-bridge2": "node scripts/generatePoseidonBridgeInput.js && ./scripts/phase2_poseidon_bridge2.sh",
    "setup:poseidon-preimage": "node scripts/generatePoseidonInput.js && ./scripts/phase2_poseidon.sh",
    "setup:poseidon3-preimage": "node scripts/generatePoseidon3Input.js && ./scripts/phase2_poseidon3.sh",
    "setup:pedersen-preimage": "node scripts/generatePedersenInput.js && ./scripts/phase2_pedersen.sh",
    "setup": "yarn setup:poseidon-bridge2 && yarn setup:poseidon-preimage && yarn setup:poseidon3-preimage",
    "test:all": "yarn build && yarn setup && yarn test",
    "test:bridge": "yarn build && yarn setup:poseidon-bridge2 && yarn test",
    "test": "npx hardhat test"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.3.1",
    "ffiasm": "^0.1.1",
    "ffjavascript": "^0.2.38",
    "hardhat": "^2.4.1",
    "snarkjs": "^0.4.6",
    "truffle-assertions": "^0.9.2",
    "web3": "^1.3.6",
    "web3-utils": "^1.2.11"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0",
    "@truffle/contract": "^4.3.24",
    "big-integer": "^1.6.48",
    "bn-chai": "^1.0.1",
    "chai-as-promised": "^7.1.1",
    "circom": "^0.5.45",
    "circom_runtime": "^0.1.9",
    "circomlib": "^0.5.2",
    "ganache-cli": "^6.12.2",
    "hardhat-artifactor": "^0.2.0",
    "ts-node": "^10.1.0",
    "hardhat-circom": "^1.0.1",
    "maci-crypto": "^0.9.1"
